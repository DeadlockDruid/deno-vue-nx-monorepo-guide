{
  "name": "@deno-vue/users",
  "version": "0.0.1",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "private": true,
  "nx": {
    "sourceRoot": "libs/shared/zod-schemas/users/src",
    "projectType": "library",
    "name": "users",
    "targets": {
      "sonar": {
        "executor": "nx:run-commands",
        "inputs": [
          "production"
        ],
        "options": {
          "command": "sonar-scanner -Dsonar.projectKey=LIB-USERS -Dsonar.projectName=LIB-USERS -Dsonar.organization=divyanshu-startup -Dsonar.sources=./src -Dsonar.tests=./src/tests -Dsonar.test.inclusions=**/*.spec.ts -Dsonar.host.url=https://sonarcloud.io -Dsonar.token=$SONAR_LIB_USERS",
          "cwd": "{projectRoot}"
        }
      }
    }
  }
}
